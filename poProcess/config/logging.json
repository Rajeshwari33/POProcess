{
	"LOGGING": {
		"version": 1,
		"disable_existing_loggers": "True",
		"loggers": {
			"po_process": {
				"handlers": ["po_process_log_file", "console"]
			}
		},
		"filters": {
			"require_debug_false": {
				"()": "django.utils.log.RequireDebugFalse"
			},
			"require_debug_true": {
				"()": "django.utils.log.RequireDebugTrue"
			}
		},
		"handlers": {
			"console": {
				"level": "INFO",
				"filters": ["require_debug_false"],
				"class": "logging.StreamHandler",
				"formatter": "simple"
			},
			"po_process_log_file": {
				"level": "INFO",
				"filters": ["require_debug_true"],
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "D:\\Adventbiz\\FinanceControlledApplication\\poProcess\\logs\\po_process.log",
				"backupCount": 5,
				"formatter": "simple"
			}
		},
		"formatters": {
			"simple": {
				"format": "[%(asctime)s] %(levelname)s %(message)s",
				"datefmt": "%Y-%m-%d %H:%M:%S"
			},
			"verbose": {
				"format": "[%(asctime)s] %(levelname)s [%(name)s.%(funcName)s:%(lineno)d] %(message)s",
				"datefmt": "%Y-%m-%d %H:%M:%S"
			}
		},
		"root": {
			"level": "INFO",
			"handlers": ["console"]
		}
	}
}